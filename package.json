    "gatsby-source-filesystem": "2.2.4",
    "gatsby-transformer-remark": "5.25.1",
    "jest": "25.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "6.0.0",
    "tailwindcss": "1.2.0",
